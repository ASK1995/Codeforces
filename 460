n = int(input())


while(n != 0):
	s = input()
	if (s[-1] == 'x' or s[-1] == 's' or s[-2:] == 'ch' or s[-1] == 'o'):
		print(s + 'es')
	elif(s[-1] == 'f'):
		print(s[:-1] + 'ves')
	elif(s[-2:] == 'fe'):
		print(s[:-2] + 'ves')
	elif(s[-1] == 'y'):
		print(s[:-1] + 'ies')
	else:
		print(s + 's')

	n -= 1